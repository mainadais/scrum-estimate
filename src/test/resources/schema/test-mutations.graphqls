mutation createPlayer($createPlayer: CreatePlayer) {
    createPlayer(createPlayer: $createPlayer){
        data{
            id
            firstName
            lastName
            emailAddress
            account{
                id
                username
            }
            role
            dateCreated
        }
    }
}

mutation addAccount($addAccount: AddAccount) {
    addAccount(addAccount: $addAccount){
        data{
            id
            firstName
            lastName
            emailAddress
            account{
                id
                username
            }
            role
            dateCreated
        }
    }
}

